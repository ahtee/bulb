{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module"
  },
  "extends": [
    "'plugin:import/recommended",
    "airbnb",
    "plugin:prettier/recommended"
  ],
  "plugins": ["react", "jsx-a11y", "react-hooks"],
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "no-console": "error",
    "no-nested-ternary": "off",
    "react/jsx-filename-extension": "off",
    "react/jsx-props-no-spreading": "off",
    "comma-dangle": "off",
    "indent": "off",
    "arrow-parens": "off"
  }
}
